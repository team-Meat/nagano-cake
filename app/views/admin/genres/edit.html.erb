<div class="row">
<h2 class="genre-edit-head">ジャンル編集</h2>
<%= form_for(@genre, url: admin_genre_path(@genre),html: {method: "patch"}) do |genre| %>
	<%= genre.text_field :name %>
	<label>
	<%= genre.radio_button :is_enabled, :true %>有効
	</label>
	<label>
	<%= genre.radio_button :is_enabled, :false %>無効
	</label>
	<%= genre.submit '保存',class:"btn btn-success genre-submit" %>
<% end %>
</div>